             xmlns:local="clr-namespace:Octarine_Core"
             StartupUri="Autorisation/Auth.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource/Colors.xaml"/>
                <ResourceDictionary Source="Resource/Texts.xaml"/>
                <ResourceDictionary Source="Resource/Ojects.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
 